{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "guard-for-in": "warn",
    "no-restricted-syntax": "warn",
    "react/jsx-filename-extension": "off",
    "jsx-a11y/href-no-hash": "off",
    "arrow-body-style": "warn",
    "react/forbid-prop-types": "warn"
  }
}
